<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY NetworkResource "http://purl.org/toco/" >
    <!ENTITY toco "https://w3id.org/toco/" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY units "https://sweet.jpl.nasa.gov/" >
    <!ENTITY time "http://www.w3.org/2006/time#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY om "http://purl.oclc.org/net/unis/ontology/sensordata.owl/" >
]>


<rdf:RDF xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://www.w3.org/2002/07/owl"
     xmlns:NetworkResource="http://purl.org/toco/"
     xmlns:toco="https://w3id.org/toco/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:time="http://www.w3.org/2006/time#"
     xmlns:units="https://sweet.jpl.nasa.gov/"
     xmlns:om="http://purl.oclc.org/net/unis/ontology/sensordata.owl/"
     xmlns:foaf="http://xmlns.com/foaf/0.1/">
    <Ontology rdf:about="http://purl.org/toco/">
        <toco:contributor rdf:datatype="&xsd;string">Mohammad Soltani Dehghani (for LiFi networks)</toco:contributor>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string">Qianru Zhou (qz1@hw.ac.uk)</rdfs:isDefinedBy>
        <toco:license rdf:datatype="&xsd;anyURI">https://www.w3.org/Consortium/Legal/2015/copyright-software-and-document</toco:license>
        <rdfs:comment>Toucan Ontology (ToCo)

A ontology developed for telecommunciation network systems with hybrid technologies, e.g., WiFi, LiFi, LTE, 2G/3G, optical, etc.</rdfs:comment>
        <versionIRI rdf:resource="http://purl.org/toco/"/>
    </Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/toco/contributor -->

    <AnnotationProperty rdf:about="&NetworkResource;contributor"/>
    


    <!-- http://purl.org/toco/license -->

    <AnnotationProperty rdf:about="&NetworkResource;license"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.oclc.org/net/unis/ontology/sensordata.owl/hasUnits -->

    <ObjectProperty rdf:about="&om;hasUnits">
        <rdfs:comment rdf:datatype="&xsd;string">reused from SenSei Observation and Measurement Ontology (http://purl.oclc.org/net/unis/ontology/sensordata.owl/).</rdfs:comment>
        <rdfs:domain rdf:resource="&om;ObervationAndMeasurement"/>
        <rdfs:range rdf:resource="&units;Units"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/associatedStations -->

    <ObjectProperty rdf:about="&NetworkResource;associatedStations">
        <rdfs:comment rdf:datatype="&xsd;string">in wireless networks, the user equipments (mobile stations in another term, e.g., phones, laptops, tablets, etc.) that are associated to the access point.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;AccessPoint"/>
        <rdfs:range rdf:resource="&NetworkResource;UserDevice"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/from -->

    <ObjectProperty rdf:about="&NetworkResource;from">
        <rdfs:comment rdf:datatype="&xsd;string">the source node that the link is transmit from.</rdfs:comment>
        <rdfs:range rdf:resource="&NetworkResource;Device"/>
        <rdfs:domain rdf:resource="&NetworkResource;Link"/>
        <inverseOf rdf:resource="&NetworkResource;to"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasBandwidth -->

    <ObjectProperty rdf:about="&NetworkResource;hasBandwidth">
        <rdfs:comment rdf:datatype="&xsd;string">the bandwidth of the link</rdfs:comment>
        <rdfs:range rdf:resource="&om;ObervationAndMeasurement"/>
        <rdfs:domain rdf:resource="&NetworkResource;Link"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasDevice -->

    <ObjectProperty rdf:about="&NetworkResource;hasDevice">
        <rdfs:domain rdf:resource="&NetworkResource;User"/>
        <rdfs:range rdf:resource="&NetworkResource;UserDevice"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasInterface -->

    <ObjectProperty rdf:about="&NetworkResource;hasInterface">
        <rdfs:domain rdf:resource="&NetworkResource;Device"/>
        <rdfs:range rdf:resource="&NetworkResource;Interface"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasLTEAssociation -->

    <ObjectProperty rdf:about="&NetworkResource;hasLTEAssociation">
        <rdfs:range rdf:resource="&NetworkResource;LTEAssociation"/>
        <rdfs:domain rdf:resource="&NetworkResource;UserEquipment"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasWirelessAssociation"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasLiFiAssociation -->

    <ObjectProperty rdf:about="&NetworkResource;hasLiFiAssociation">
        <rdfs:range rdf:resource="&NetworkResource;LiFiAssociation"/>
        <rdfs:domain rdf:resource="&NetworkResource;LiFiWLAN"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasWirelessAssociation"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasLiFiWLAN -->

    <ObjectProperty rdf:about="&NetworkResource;hasLiFiWLAN">
        <rdfs:domain rdf:resource="&NetworkResource;Device"/>
        <rdfs:range rdf:resource="&NetworkResource;LiFiWLAN"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasWLAN"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasLink -->

    <ObjectProperty rdf:about="&NetworkResource;hasLink">
        <rdfs:domain rdf:resource="&NetworkResource;Interface"/>
        <rdfs:range rdf:resource="&NetworkResource;Link"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasPort -->

    <ObjectProperty rdf:about="&NetworkResource;hasPort">
        <rdfs:domain rdf:resource="&NetworkResource;Device"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasInterface"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasSatelliteAssociation -->

    <ObjectProperty rdf:about="&NetworkResource;hasSatelliteAssociation">
        <rdfs:domain rdf:resource="&NetworkResource;Device"/>
        <rdfs:range rdf:resource="&NetworkResource;SatelliteAssociation"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasWirelessAssociation"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasService -->

    <ObjectProperty rdf:about="&NetworkResource;hasService">
        <rdfs:range rdf:resource="&NetworkResource;Service"/>
        <rdfs:domain rdf:resource="&NetworkResource;UserDevice"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasWLAN -->

    <ObjectProperty rdf:about="&NetworkResource;hasWLAN">
        <rdfs:domain rdf:resource="&NetworkResource;Device"/>
        <rdfs:range rdf:resource="&NetworkResource;WLAN"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasInterface"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasWiFiAssociation -->

    <ObjectProperty rdf:about="&NetworkResource;hasWiFiAssociation">
        <rdfs:range rdf:resource="&NetworkResource;WiFiAssociation"/>
        <rdfs:domain rdf:resource="&NetworkResource;WiFiWLAN"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasWirelessAssociation"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasWiFiWLAN -->

    <ObjectProperty rdf:about="&NetworkResource;hasWiFiWLAN">
        <rdfs:domain rdf:resource="&NetworkResource;Device"/>
        <rdfs:range rdf:resource="&NetworkResource;WiFiWLAN"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasWLAN"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasWiredLink -->

    <ObjectProperty rdf:about="&NetworkResource;hasWiredLink">
        <rdfs:domain rdf:resource="&NetworkResource;Interface"/>
        <rdfs:range rdf:resource="&NetworkResource;WiredLink"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasLink"/>
        <propertyDisjointWith rdf:resource="&NetworkResource;hasWirelessAssociation"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/hasWirelessAssociation -->

    <ObjectProperty rdf:about="&NetworkResource;hasWirelessAssociation">
        <rdfs:domain rdf:resource="&NetworkResource;WLAN"/>
        <rdfs:range rdf:resource="&NetworkResource;WirelessAssociation"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasLink"/>
    </ObjectProperty>
    


    <!-- http://purl.org/toco/to -->

    <ObjectProperty rdf:about="&NetworkResource;to">
        <rdfs:comment rdf:datatype="&xsd;string">the destination node of the link.</rdfs:comment>
        <rdfs:range rdf:resource="&NetworkResource;Device"/>
        <rdfs:domain rdf:resource="&NetworkResource;Link"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.oclc.org/net/unis/ontology/sensordata.owl/hasUnit -->

    <DatatypeProperty rdf:about="&om;hasUnit">
        <rdfs:comment rdf:datatype="&xsd;string">reused from SENSEI Observation and Measurement Ontology.</rdfs:comment>
        <rdfs:domain rdf:resource="&om;ObervationAndMeasurement"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </DatatypeProperty>
    


    <!-- http://purl.oclc.org/net/unis/ontology/sensordata.owl/hasValue -->

    <DatatypeProperty rdf:about="&om;hasValue">
        <rdfs:comment rdf:datatype="&xsd;string">reused from SENSEI observation and measurement ontology.</rdfs:comment>
        <rdfs:domain rdf:resource="&om;ObervationAndMeasurement"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasAntennaGain -->

    <DatatypeProperty rdf:about="&NetworkResource;hasAntennaGain">
        <rdfs:domain rdf:resource="&NetworkResource;WLAN"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasInterfaceProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasAntennaHeight -->

    <DatatypeProperty rdf:about="&NetworkResource;hasAntennaHeight">
        <rdfs:domain rdf:resource="&NetworkResource;WLAN"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasInterfaceProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasConnectionEndTime -->

    <DatatypeProperty rdf:about="&NetworkResource;hasConnectionEndTime">
        <rdfs:domain rdf:resource="&NetworkResource;Service"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasServiceProperty"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasConnectionStartTime -->

    <DatatypeProperty rdf:about="&NetworkResource;hasConnectionStartTime">
        <rdfs:domain rdf:resource="&NetworkResource;Service"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasServiceProperty"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasDistance -->

    <DatatypeProperty rdf:about="&NetworkResource;hasDistance">
        <rdfs:comment rdf:datatype="&xsd;string">the Euclidean distance between the LiFi user equipment and access point.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;LiFiAssociation"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasLiFiAssociationProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasEndTime -->

    <DatatypeProperty rdf:about="&NetworkResource;hasEndTime">
        <rdfs:comment rdf:datatype="&xsd;string">the time that a service ends.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;Service"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasServiceProperty"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasErrorCode -->

    <DatatypeProperty rdf:about="&NetworkResource;hasErrorCode">
        <rdfs:comment rdf:datatype="&xsd;string">the error code of a service.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;Service"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasServiceProperty"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasFieldOfView -->

    <DatatypeProperty rdf:about="&NetworkResource;hasFieldOfView">
        <rdfs:comment rdf:datatype="&xsd;string">Field of view (FOV) is the open observable area where a LiFi user equipment could receive visiable light signal. It is defined by let the incident angle of the LiFi user equipment equals to Pi/2.

If the incident angle larger than Pi/2, the light ray is out of the FOV, thus cannot be received by the LiFi user equipment.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;LiFiUserEuipment"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasLiFiUserEquipmentProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasGainOfConcentrator -->

    <DatatypeProperty rdf:about="&NetworkResource;hasGainOfConcentrator">
        <rdfs:comment rdf:datatype="&xsd;string">The gain of the concentrator on the LiFi user equipment, usually equals to 1.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;LiFiUserEuipment"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasLiFiUserEquipmentProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasGainofOpticalFilter -->

    <DatatypeProperty rdf:about="&NetworkResource;hasGainofOpticalFilter">
        <rdfs:comment rdf:datatype="&xsd;string">Gain of optical filter on LiFi access point.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;LiFiAccessPoint"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasLiFiAccessPointProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasHalfIntensityAngle -->

    <DatatypeProperty rdf:about="&NetworkResource;hasHalfIntensityAngle">
        <rdfs:comment rdf:datatype="&xsd;string">The angle of half intensity of the LED light transmiting visible light signal in a LiFi access point. 

A fixed value. It is the angle where the radiated intensity will be half as intense as it would be at 0 degrees, or pointing straight ahead.

Factors that contribute to the angle of half intensity include the amount of diffusing material in the epoxy, the shape of the reflector cup which surrounds the LED chip, the shape of the LED lens, the distance from the LED to the tip of the lens, and the type of emitter chip.

The most common value is Pi/3.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;LiFiAccessPoint"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasLiFiAccessPointProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasIP -->

    <DatatypeProperty rdf:about="&NetworkResource;hasIP">
        <rdfs:comment rdf:datatype="&xsd;string">the IP address of the interface.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;Interface"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasInterfaceProperty"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasIncidentAngle -->

    <DatatypeProperty rdf:about="&NetworkResource;hasIncidentAngle">
        <rdfs:comment rdf:datatype="&xsd;string">The angle of incidence of the LED receiver on LiFi user equipment, which is the angle between the visiable light ray incident on a surface and the line perpendicular to the surface at the point of incidence, called the normal.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;LiFiAssociation"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasLiFiAssociationProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasInterfaceProperty -->

    <DatatypeProperty rdf:about="&NetworkResource;hasInterfaceProperty">
        <rdfs:domain rdf:resource="&NetworkResource;Interface"/>
        <rdfs:range rdf:resource="&xsd;float"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasLiFiAccessPointProperty -->

    <DatatypeProperty rdf:about="&NetworkResource;hasLiFiAccessPointProperty">
        <rdfs:comment rdf:datatype="&xsd;string">datatype properties dedicated for LiFi access points</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;LiFiAccessPoint"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasLiFiAssociationProperty -->

    <DatatypeProperty rdf:about="&NetworkResource;hasLiFiAssociationProperty">
        <rdfs:comment rdf:datatype="&xsd;string">The properties of the visiable light association between the LED on LiFi access point and LiFi user equipment.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;LiFiAssociation"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasWirelessLinkProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasLiFiUserEquipmentProperty -->

    <DatatypeProperty rdf:about="&NetworkResource;hasLiFiUserEquipmentProperty">
        <rdfs:comment rdf:datatype="&xsd;string">properties of LiFi user equipments.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;LiFiUserEuipment"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasLinkProperty -->

    <DatatypeProperty rdf:about="&NetworkResource;hasLinkProperty">
        <rdfs:domain rdf:resource="&NetworkResource;Link"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasMAC -->

    <DatatypeProperty rdf:about="&NetworkResource;hasMAC">
        <rdfs:comment rdf:datatype="&xsd;string">the MAC address of the interface.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;Interface"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasInterfaceProperty"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasName -->

    <DatatypeProperty rdf:about="&NetworkResource;hasName">
        <rdfs:domain rdf:resource="&NetworkResource;Interface"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasInterfaceProperty"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasOpticalTransmittedPower -->

    <DatatypeProperty rdf:about="&NetworkResource;hasOpticalTransmittedPower">
        <rdfs:comment rdf:datatype="&xsd;string">the transmitted power of the LED on the LiFi access point</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;LiFiAccessPoint"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasLiFiAccessPointProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasRadianceAngle -->

    <DatatypeProperty rdf:about="&NetworkResource;hasRadianceAngle">
        <rdfs:comment rdf:datatype="&xsd;string">the radiance angle of the light ray transmitted from LED on LiFi access point.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;LiFiAssociation"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasLiFiAssociationProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasRespansivity -->

    <DatatypeProperty rdf:about="&NetworkResource;hasRespansivity">
        <rdfs:domain rdf:resource="&NetworkResource;LiFiAccessPoint"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasLiFiAccessPointProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasServiceProperty -->

    <DatatypeProperty rdf:about="&NetworkResource;hasServiceProperty">
        <rdfs:domain rdf:resource="&NetworkResource;Service"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasServiceStatus -->

    <DatatypeProperty rdf:about="&NetworkResource;hasServiceStatus">
        <rdfs:comment rdf:datatype="&xsd;string">the status of the service.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;Service"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasServiceProperty"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasServiceType -->

    <DatatypeProperty rdf:about="&NetworkResource;hasServiceType">
        <rdfs:comment rdf:datatype="&xsd;string">the type of communication services, e.g., video, audio, file transmit, text transmiting, etc.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;Service"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasServiceProperty"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasStartTime -->

    <DatatypeProperty rdf:about="&NetworkResource;hasStartTime">
        <rdfs:comment rdf:datatype="&xsd;string">the start time of a service.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;Service"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasServiceProperty"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasTransmitPower -->

    <DatatypeProperty rdf:about="&NetworkResource;hasTransmitPower">
        <rdfs:comment rdf:datatype="&xsd;string">the power of the transmitter on the interface.</rdfs:comment>
        <rdfs:domain rdf:resource="&NetworkResource;Interface"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasInterfaceProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/toco/hasWirelessLinkProperty -->

    <DatatypeProperty rdf:about="&NetworkResource;hasWirelessLinkProperty">
        <rdfs:domain rdf:resource="&NetworkResource;WirelessAssociation"/>
        <rdfs:subPropertyOf rdf:resource="&NetworkResource;hasLinkProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.oclc.org/net/unis/ontology/sensordata.owl/ObervationAndMeasurement -->

    <Class rdf:about="&om;ObervationAndMeasurement">
        <rdfs:subClassOf rdf:resource="&NetworkResource;ConceptEntities"/>
        <rdfs:comment rdf:datatype="&xsd;string">reused from SenSei Observation And Measurement Ontology (http://purl.oclc.org/net/unis/ontology/sensordata.owl).</rdfs:comment>
    </Class>
    


    <!-- http://purl.org/toco/AccessPoint -->

    <Class rdf:about="&NetworkResource;AccessPoint">
        <rdfs:subClassOf rdf:resource="&NetworkResource;SystemDevice"/>
        <rdfs:comment rdf:datatype="&xsd;string">a wireless access point, or more generally just access point (AP), is a networking hardware device that allows a Wi-Fi/Li-Fi device to connect to a wired network. The AP usually connects to a router (via a wired network) as a standalone device, but it can also be an integral component of the router itself. An AP is differentiated from a hotspot, which is the physical location where Wi-Fi access to a WLAN is available.</rdfs:comment>
    </Class>
    


    <!-- http://purl.org/toco/AudioSession -->

    <Class rdf:about="&NetworkResource;AudioSession">
        <rdfs:subClassOf rdf:resource="&NetworkResource;Session"/>
    </Class>
    


    <!-- http://purl.org/toco/BaseStation -->

    <Class rdf:about="&NetworkResource;BaseStation">
        <rdfs:subClassOf rdf:resource="&NetworkResource;SystemDevice"/>
        <rdfs:comment rdf:datatype="&xsd;string">a &quot;land station in the land mobile service.&quot; -- according to the International Telecommunication Union&apos;s (ITU) Radio Regulations (RR).

it is a transceiver connecting a number of other devices to one another and/or to a wider area. In LTE, it is also known as eNodeB.</rdfs:comment>
    </Class>
    


    <!-- http://purl.org/toco/ConceptEntities -->


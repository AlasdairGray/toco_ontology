<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY toco "http://purl.org/toco/" >
    <!ENTITY toco2 "https://w3id.org/toco/" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY units "https://sweet.jpl.nasa.gov/" >
    <!ENTITY time "http://www.w3.org/2006/time#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY geo "http://www.w3.org/2003/01/geo/wgs84_pos/" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY om "http://purl.oclc.org/net/unis/ontology/sensordata.owl/" >
]>


<rdf:RDF xmlns="http://purl.org/toco/"
     xml:base="http://purl.org/toco/"
     xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos/"
     xmlns:toco="http://purl.org/toco/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:time="http://www.w3.org/2006/time#"
     xmlns:units="https://sweet.jpl.nasa.gov/"
     xmlns:toco2="https://w3id.org/toco/"
     xmlns:om="http://purl.oclc.org/net/unis/ontology/sensordata.owl/"
     xmlns:foaf="http://xmlns.com/foaf/0.1/">
    <owl:Ontology rdf:about="http://purl.org/toco/">
        <toco2:contributor rdf:datatype="&xsd;string">Mohammad Soltani Dehghani (for LiFi networks)</toco2:contributor>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string">Qianru Zhou (qz1@hw.ac.uk)</rdfs:isDefinedBy>
        <toco2:license rdf:datatype="&xsd;anyURI">https://www.w3.org/Consortium/Legal/2015/copyright-software-and-document</toco2:license>
        <rdfs:comment>Toucan Ontology (ToCo)

A ontology developed for telecommunciation network systems with hybrid technologies, e.g., WiFi, LiFi, LTE, 2G/3G, optical, etc.</rdfs:comment>
        <owl:versionIRI rdf:resource="http://purl.org/toco/"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/toco/contributor -->

    <owl:AnnotationProperty rdf:about="&toco;contributor"/>
    


    <!-- http://purl.org/toco/license -->

    <owl:AnnotationProperty rdf:about="&toco;license"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.oclc.org/net/unis/ontology/sensordata.owl/hasUnits -->

    <owl:ObjectProperty rdf:about="&om;hasUnits">
        <rdfs:comment rdf:datatype="&xsd;string">reused from SenSei Observation and Measurement Ontology (http://purl.oclc.org/net/unis/ontology/sensordata.owl/).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/net/unis/ontology/sensordata.owl/"/>
        <rdfs:domain rdf:resource="&om;ObervationAndMeasurement"/>
        <rdfs:range rdf:resource="&units;Units"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/APsInRange -->

    <owl:ObjectProperty rdf:about="&toco;APsInRange">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:range rdf:resource="&toco;AccessPoint"/>
        <rdfs:domain rdf:resource="&toco;UserDevice"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/BelongsTo -->

    <owl:ObjectProperty rdf:about="&toco;BelongsTo">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:range rdf:resource="&toco;User"/>
        <rdfs:domain rdf:resource="&toco;UserDevice"/>
        <owl:inverseOf rdf:resource="&toco;hasDevice"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/From -->

    <owl:ObjectProperty rdf:about="&toco;From">
        <rdfs:comment rdf:datatype="&xsd;string">the source node that the link is transmit from.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:range rdf:resource="&toco;Interface"/>
        <rdfs:domain rdf:resource="&toco;Link"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/To -->

    <owl:ObjectProperty rdf:about="&toco;To">
        <rdfs:comment rdf:datatype="&xsd;string">the destination node of the link.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:range rdf:resource="&toco;Interface"/>
        <rdfs:domain rdf:resource="&toco;Link"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasAssociatedStations -->

    <owl:ObjectProperty rdf:about="&toco;hasAssociatedStations">
        <rdfs:comment rdf:datatype="&xsd;string">in wireless networks, the user equipments (mobile stations in another term, e.g., phones, laptops, tablets, etc.) that are associated to the access point.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;AccessPoint"/>
        <rdfs:range rdf:resource="&toco;UserDevice"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasBandwidth -->

    <owl:ObjectProperty rdf:about="&toco;hasBandwidth">
        <rdfs:comment rdf:datatype="&xsd;string">the bandwidth of the link</rdfs:comment>
        <rdfs:range rdf:resource="&om;ObervationAndMeasurement"/>
        <rdfs:domain rdf:resource="&toco;Link"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasLinkProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasDevice -->

    <owl:ObjectProperty rdf:about="&toco;hasDevice">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;User"/>
        <rdfs:range rdf:resource="&toco;UserDevice"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasFlow -->

    <owl:ObjectProperty rdf:about="&toco;hasFlow">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:range rdf:resource="&toco;Flow"/>
        <rdfs:domain rdf:resource="&toco;SystemDevice"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasFlowAction -->

    <owl:ObjectProperty rdf:about="&toco;hasFlowAction">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:range rdf:resource="&toco;Action"/>
        <rdfs:domain rdf:resource="&toco;Flow"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasInterface -->

    <owl:ObjectProperty rdf:about="&toco;hasInterface">
        <rdfs:comment rdf:datatype="&xsd;string">the fact that a device has a interface.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;Device"/>
        <rdfs:range rdf:resource="&toco;Interface"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasLTEAssociation -->

    <owl:ObjectProperty rdf:about="&toco;hasLTEAssociation">
        <rdfs:range rdf:resource="&toco;LTEAssociation"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasWirelessAssociation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasLiFiAssociation -->

    <owl:ObjectProperty rdf:about="&toco;hasLiFiAssociation">
        <rdfs:range rdf:resource="&toco;LiFiAssociation"/>
        <rdfs:domain rdf:resource="&toco;LiFiWLAN"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasWirelessAssociation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasLiFiWLAN -->

    <owl:ObjectProperty rdf:about="&toco;hasLiFiWLAN">
        <rdfs:domain rdf:resource="&toco;Device"/>
        <rdfs:range rdf:resource="&toco;LiFiWLAN"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasWLAN"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasLink -->

    <owl:ObjectProperty rdf:about="&toco;hasLink">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;Interface"/>
        <rdfs:range rdf:resource="&toco;Link"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasLinkProperty -->

    <owl:ObjectProperty rdf:about="&toco;hasLinkProperty"/>
    


    <!-- http://purl.org/toco/hasPort -->

    <owl:ObjectProperty rdf:about="&toco;hasPort">
        <rdfs:domain rdf:resource="&toco;Device"/>
        <rdfs:range rdf:resource="&toco;Interface"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasInterface"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasRoundTripTime -->

    <owl:ObjectProperty rdf:about="&toco;hasRoundTripTime">
        <rdfs:comment rdf:datatype="&xsd;string">the round trip time of a link, which is the time taken for signal travel a round trip via this link.</rdfs:comment>
        <rdfs:range rdf:resource="&om;ObervationAndMeasurement"/>
        <rdfs:domain rdf:resource="&toco;Link"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasLinkProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasSatelliteAssociation -->

    <owl:ObjectProperty rdf:about="&toco;hasSatelliteAssociation">
        <rdfs:domain rdf:resource="&toco;Device"/>
        <rdfs:range rdf:resource="&toco;SatelliteAssociation"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasWirelessAssociation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasService -->

    <owl:ObjectProperty rdf:about="&toco;hasService">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:range rdf:resource="&toco;Service"/>
        <rdfs:domain rdf:resource="&toco;UserDevice"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasStandardDeviation -->

    <owl:ObjectProperty rdf:about="&toco;hasStandardDeviation">
        <rdfs:comment rdf:datatype="&xsd;string">After multiple round trip time tested on a link, the deviation of these test results.</rdfs:comment>
        <rdfs:range rdf:resource="&om;ObervationAndMeasurement"/>
        <rdfs:domain rdf:resource="&toco;Link"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasLinkProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasThroughPut -->

    <owl:ObjectProperty rdf:about="&toco;hasThroughPut">
        <rdfs:range rdf:resource="&om;ObervationAndMeasurement"/>
        <rdfs:domain rdf:resource="&toco;Link"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasLinkProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasTransmitPower -->

    <owl:ObjectProperty rdf:about="&toco;hasTransmitPower">
        <rdfs:range rdf:resource="&om;ObervationAndMeasurement"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;Interface"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasWLAN -->

    <owl:ObjectProperty rdf:about="&toco;hasWLAN">
        <rdfs:domain rdf:resource="&toco;Device"/>
        <rdfs:range rdf:resource="&toco;WLAN"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasInterface"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasWiFiAssociation -->

    <owl:ObjectProperty rdf:about="&toco;hasWiFiAssociation">
        <rdfs:range rdf:resource="&toco;WiFiAssociation"/>
        <rdfs:domain rdf:resource="&toco;WiFiWLAN"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasWirelessAssociation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasWiFiWLAN -->

    <owl:ObjectProperty rdf:about="&toco;hasWiFiWLAN">
        <rdfs:domain rdf:resource="&toco;Device"/>
        <rdfs:range rdf:resource="&toco;WiFiWLAN"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasWLAN"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasWiredLink -->

    <owl:ObjectProperty rdf:about="&toco;hasWiredLink">
        <rdfs:domain rdf:resource="&toco;Interface"/>
        <rdfs:range rdf:resource="&toco;WiredLink"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasLink"/>
        <owl:propertyDisjointWith rdf:resource="&toco;hasWirelessAssociation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/hasWirelessAssociation -->

    <owl:ObjectProperty rdf:about="&toco;hasWirelessAssociation">
        <rdfs:domain rdf:resource="&toco;WLAN"/>
        <rdfs:range rdf:resource="&toco;WirelessAssociation"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasLink"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/isIn -->

    <owl:ObjectProperty rdf:about="&toco;isIn">
        <rdfs:comment rdf:datatype="&xsd;string">the relation that a interface belongs to a device. A device can have multiple interfaces, but a interface can be in one and only one device.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:range rdf:resource="&toco;Device"/>
        <rdfs:domain rdf:resource="&toco;Interface"/>
        <owl:inverseOf rdf:resource="&toco;hasInterface"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/range -->

    <owl:ObjectProperty rdf:about="&toco;range">
        <rdfs:comment rdf:datatype="&xsd;string">the signal range of wireless devices, such as WiFi access point, LiFi access point, WiFi mobile station, and LiFi mobile station.</rdfs:comment>
        <rdfs:range rdf:resource="&om;ObervationAndMeasurement"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;Device"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/stationsInRange -->

    <owl:ObjectProperty rdf:about="&toco;stationsInRange">
        <rdfs:comment rdf:datatype="&xsd;string">in wireless networks (WiFi/LiFi), the mobile stations in the range of the access points.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;AccessPoint"/>
        <rdfs:range rdf:resource="&toco;UserEquipment"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/toco/toPort -->

    <owl:ObjectProperty rdf:about="&toco;toPort">
        <rdfs:comment>the port to send to, defined in an flow action &quot;Output&quot; and &quot;Forward&quot;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;Action"/>
        <rdfs:range rdf:resource="&toco;Interface"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#location -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#location">
        <rdfs:comment>The relation between something and the point, 
 or other geometrical thing in space, where it is.  For example, the realtionship between
 a radio tower and a Point with a given lat and long.
 Or a relationship between a park and its outline as a closed arc of points, or a road and
 its location as a arc (a sequence of points).
 Clearly in practice there will be limit to the accuracy of any such statement, but one would expect
 an accuracy appropriate for the size of the object and uses such as mapping .</rdfs:comment>
        <rdfs:domain rdf:resource="&toco;Device"/>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#"/>
        <rdfs:range rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpartialThing"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.oclc.org/net/unis/ontology/sensordata.owl/hasUnit -->

    <owl:DatatypeProperty rdf:about="&om;hasUnit">
        <rdfs:comment rdf:datatype="&xsd;string">reused from SENSEI Observation and Measurement Ontology.</rdfs:comment>
        <rdfs:domain rdf:resource="&om;ObervationAndMeasurement"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.oclc.org/net/unis/ontology/sensordata.owl/hasValue -->

    <owl:DatatypeProperty rdf:about="&om;hasValue">
        <rdfs:comment rdf:datatype="&xsd;string">reused from SENSEI observation and measurement ontology.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/net/unis/ontology/sensordata.owl/"/>
        <rdfs:domain rdf:resource="&om;ObervationAndMeasurement"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/toco/Channel -->

    <owl:DatatypeProperty rdf:about="&toco;Channel">
        <rdfs:comment rdf:datatype="&xsd;string">the channel id of a interface.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;Interface"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/toco/Frequency -->

    <owl:DatatypeProperty rdf:about="&toco;Frequency">
        <rdfs:comment rdf:datatype="&xsd;string">the frequency of the radio wave transmitted by a wireless interface (WLAN).</rdfs:comment>
        <rdfs:domain rdf:resource="&toco;Interface"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/toco/cookie -->

    <owl:DatatypeProperty rdf:about="&toco;cookie">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;Flow"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasFlowProperty"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/toco/flags -->

    <owl:DatatypeProperty rdf:about="&toco;flags">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;Flow"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasFlowProperty"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/toco/hard_timeout -->

    <owl:DatatypeProperty rdf:about="&toco;hard_timeout">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;Flow"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasFlowProperty"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/toco/hasAntennaGain -->

    <owl:DatatypeProperty rdf:about="&toco;hasAntennaGain">
        <rdfs:comment rdf:datatype="&xsd;string">the gain of an antenna on the wireless interface.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;WLAN"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/toco/hasAntennaHeight -->

    <owl:DatatypeProperty rdf:about="&toco;hasAntennaHeight">
        <rdfs:comment rdf:datatype="&xsd;string">the height of the antenna of a wireless interface</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;WLAN"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/toco/hasDistance -->

    <owl:DatatypeProperty rdf:about="&toco;hasDistance">
        <rdfs:comment rdf:datatype="&xsd;string">the Euclidean distance between the LiFi user equipment and access point.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;LiFiAssociation"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasLiFiAssociationProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/toco/hasErrorCode -->

    <owl:DatatypeProperty rdf:about="&toco;hasErrorCode">
        <rdfs:comment rdf:datatype="&xsd;string">the error code of a service.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;Service"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/toco/hasFieldOfView -->

    <owl:DatatypeProperty rdf:about="&toco;hasFieldOfView">
        <rdfs:comment rdf:datatype="&xsd;string">Field of view (FOV) is the open observable area where a LiFi user equipment could receive visiable light signal. It is defined by let the incident angle of the LiFi user equipment equals to Pi/2.

If the incident angle larger than Pi/2, the light ray is out of the FOV, thus cannot be received by the LiFi user equipment.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;LiFiUserEquipment"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasLiFiUserEquipmentProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/toco/hasFlowProperty -->

    <owl:DatatypeProperty rdf:about="&toco;hasFlowProperty">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/toco/hasGainOfConcentrator -->

    <owl:DatatypeProperty rdf:about="&toco;hasGainOfConcentrator">
        <rdfs:comment rdf:datatype="&xsd;string">The gain of the concentrator on the LiFi user equipment, usually equals to 1.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;LiFiUserEquipment"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasLiFiUserEquipmentProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/toco/hasGainofOpticalFilter -->

    <owl:DatatypeProperty rdf:about="&toco;hasGainofOpticalFilter">
        <rdfs:comment rdf:datatype="&xsd;string">Gain of optical filter on LiFi access point.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;LiFiAccessPoint"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasLiFiAccessPointProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/toco/hasHalfIntensityAngle -->

    <owl:DatatypeProperty rdf:about="&toco;hasHalfIntensityAngle">
        <rdfs:comment rdf:datatype="&xsd;string">The angle of half intensity of the LED light transmiting visible light signal in a LiFi access point. 

A fixed value. It is the angle where the radiated intensity will be half as intense as it would be at 0 degrees, or pointing straight ahead.

Factors that contribute to the angle of half intensity include the amount of diffusing material in the epoxy, the shape of the reflector cup which surrounds the LED chip, the shape of the LED lens, the distance from the LED to the tip of the lens, and the type of emitter chip.

The most common value is Pi/3.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/toco/"/>
        <rdfs:domain rdf:resource="&toco;LiFiAccessPoint"/>
        <rdfs:subPropertyOf rdf:resource="&toco;hasLiFiAccessPointProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


